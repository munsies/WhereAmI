<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!--Import gazebo elements-->
  <xacro:include filename="$(find my_robot_2)/urdf/my_robot_2.gazebo" />

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0.1" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="15.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".5 .3 .1"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".5 .3 .1"/>
      </geometry>
    </visual>

  </link>

  <link name='front_left_wheel'>
    <pose>0 0 0.0 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </collision>

    <visual name='front_left_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </visual>

  </link>

  <joint type="continuous" name="front_left_wheel_hinge">
    <origin xyz="0.25 0.2 0" rpy="0 0 0"/>
    <child link="front_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name='front_right_wheel'>
    <pose>0 0 0.0 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </collision>

    <visual name='front_right_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </visual>

  </link>

  <joint type="continuous" name="front_right_wheel_hinge">
    <origin xyz="0.25 -0.2 0" rpy="0 0 0"/>
    <child link="front_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name='back_left_wheel'>
    <pose>0 0 0.0 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </collision>

    <visual name='back_left_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </visual>

  </link>

  <joint type="continuous" name="back_left_wheel_hinge">
    <origin xyz="-0.25 0.2 0" rpy="0 0 0"/>
    <child link="back_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name='back_right_wheel'>
    <pose>0 0 0.0 0 0 0</pose>

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </collision>

    <visual name='back_right_wheel_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="0.1" length ="0.05"/>
      </geometry>
    </visual>

  </link>

  <joint type="continuous" name="back_right_wheel_hinge">
    <origin xyz="-0.25 -0.2 0" rpy="0 0 0"/>
    <child link="back_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name='camera'>
    <pose>0 0 0.0 0 0 0</pose>

    <inertial>
      <mass value="0.1"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
    </collision>

    <visual name='camera_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
    </visual>

  </link>

  <joint name="camera_joint" type="fixed">
    <axis xyz="0 1 0"/>
    <origin xyz="0.25 0 0" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="camera"/>
  </joint>

  <link name='hokuyo'>
    <pose>0 0 0.0 0 0 0</pose>

    <inertial>
      <mass value="1e-5"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="1e-6" ixy="0" ixz="0"
          iyy="1e-6" iyz="0"
          izz="1e-6"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".1 .1 .1"/>
      </geometry>
    </collision>

    <visual name='hokuyo_visual'>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <mesh filename="package://my_robot_2/meshes/hokuyo.dae"/>
      </geometry>
    </visual>

  </link>

  <joint name="hokuyo_joint" type="fixed">
    <axis xyz="0 1 0"/>
    <origin xyz="0.15 0 0.15" rpy="0 0 0" />
    <parent link="chassis"/>
    <child link="hokuyo"/>
  </joint>

  <gazebo reference="front_left_wheel">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="front_right_wheel">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="back_left_wheel">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="back_right_wheel">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="chassis">
    <material>Gazebo/Red</material>
  </gazebo>

  <gazebo reference="camera">
    <material>Gazebo/Blue</material>
  </gazebo> 

  <gazebo reference="front_left_wheel">
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <fdir1>1 1 0</fdir1>
    <slip1>0.5</slip1>
    <slip2>0.0</slip2>
  </gazebo> 

  <gazebo reference="front_right_wheel">
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <fdir1>1 1 0</fdir1>
    <slip1>0.5</slip1>
    <slip2>0.0</slip2>
  </gazebo> 

  <gazebo reference="rear_left_wheel">
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <fdir1>1 1 0</fdir1>
    <slip1>0.5</slip1>
    <slip2>0.0</slip2>
  </gazebo> 

  <gazebo reference="rear_right_wheel">
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <fdir1>1 1 0</fdir1>
    <slip1>0.5</slip1>
    <slip2>0.0</slip2>
  </gazebo>

</robot>
